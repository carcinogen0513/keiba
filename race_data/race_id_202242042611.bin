});
});
$(document).ready(function() {
// 予想家コメント全文表示ボタン
open_comment();
});
function open_comment() {
var $opners = $('.textOpener');
if ($opners.length > 0) {
$opners.on('click', function() {
var $text = $(this).prev('p');
if ($text.hasClass('expanded')) {
//閉じる処理
$text.removeAttr('style').removeClass('expanded');
$(this).text('もっと見る');
} else {
//展開する処理
var height = $text.get(0).scrollHeight;
$text.css('height', height).addClass('expanded');
$(this).text('閉じる');
}
});
}
}
