});
// 非表示・通報の使い方について 開閉
$('.ExpansionPanel dt').click(function() {
$(this).next().slideToggle('fast');
$Icon = $(this).find('span');
if ($Icon.hasClass('Icon_ArrowB')) {
$Icon.removeClass('Icon_ArrowB').addClass('Icon_ArrowT');
} else {
$Icon.removeClass('Icon_ArrowT').addClass('Icon_ArrowB');
}
});
// コメント返信機能 clickイベント
$('.CommentResBtn').click(function(e) {
e.preventDefault();
$Anchor = $(this).parents('li').children('.Anchor');
itemId = $Anchor.attr('id').replace('comment_', '');
$('#commentForm').val('>>' + itemId + '\n');
showCommentForm();
$('html,body').animate({
scrollTop: $('#comment').offset().top
}, 0);
});
// コメントミュート機能 clickイベント
$('.CommentMuteBtn').click(function() {
$(this).nextAll('.CommentMuteToggle:first').slideToggle('fast');
});
/*メニュー開閉*/
$('#PanelOpen').on('click', function() {
